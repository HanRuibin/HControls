<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:control="clr-namespace:xView.Controls.Controls"
    >
    <LinearGradientBrush x:Key="back" EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
        <GradientStop Color="#FF606060" Offset="0"/>
        <GradientStop Color="#FF4F4F4F" Offset="1"/>
        <GradientStop Color="#FF424242" Offset="0.43"/>
    </LinearGradientBrush>
    <Style TargetType="control:CanvasTimeLine">
        <Setter Property="Background" Value="{StaticResource back}"/>
    </Style>
    <Style TargetType="Slider" x:Key="UCSeekStyle">
        
    </Style>
</ResourceDictionary>
